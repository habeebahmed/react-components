{
  "name": "react-components-test-hm",
  "version": "0.0.0-development",
  "description": "react-components-test-hm/ React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build --copy-files --no-demo",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "prepublishOnly": "yarn run build",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "acp":"git add . && npm run commit && git push"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "nwb": "0.25.x",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "semantic-release": "^19.0.2"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/habeebahmed/react-components-test-hm.git"
  },
  "keywords": [
    "react-component"
  ],
  "dependencies": {
    "semantic-release-cli": "^5.4.4"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
